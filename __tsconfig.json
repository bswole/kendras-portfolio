{
  "compilerOptions": {
    "target": "es5",
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true
  },
  "include": ["next-env.d.ts", "**/*.ts", "**/*.tsx", "lib/slides.js", "src/pages/case-studies.js", "src/pages/designs.js", "src/pages/index.js", "src/pages/resume.js", "src/pages/slide-show.js", "src/components/footer.js", "src/components/intro.js", "src/components/slideShow.jsx"],
  "exclude": ["node_modules"]
}
